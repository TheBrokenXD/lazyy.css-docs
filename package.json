{
  "name": "broken-lazy-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lazyy": "sass  --no-source-map styles/index.scss:styles/index.css",
    "watch:lazyy": "sass  --no-source-map styles/index.scss:styles/index.css --watch",
    "purge:lazyy": "purgecss --css styles/index.css --content \"pages/**/*.js\" --output styles/",
    "build:lazyy": "npm run purge:lazyy && postcss styles/index.css -u autoprefixer cssnano -r --no-map"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.4",
    "chokidar": "^3.5.3",
    "cssnano": "^5.1.7",
    "framer-motion": "^6.2.8",
    "lottie-react": "^2.2.1",
    "next": "12.1.1",
    "nprogress": "^0.2.0",
    "postcss-cli": "^9.1.0",
    "prismjs": "^1.27.0",
    "purgecss": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "sass": "^1.50.0"
  }
}
